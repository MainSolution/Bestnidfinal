<h1> Articulos </h1>
<table class="table">	
	<tr>
		 <th> Nombre </th>	
		 <th> Duracion </th>
		 <th> Foto </th>
		 <th><a href="/articles/new" class="btn btn-danger" role="button">Subastar</a></th>

    </tr>
   
    <% @articulos.each do |a| %>  	
 		<tr>	
			 <td> <%= link_to a.nombre, article_path(a) %> </td>	
			 <td> <%= a.duracion %>    </td>
			 <td> <%= image_tag a.foto , :width => 70, :height => 70 %> </td>
			 <td> <%= link_to "Editar", edit_article_path(a) %> </td>	
			 <td> <%= link_to "Eliminar", a,method: :delete %> </td>
		</tr>	
	<% end %>	
</table>